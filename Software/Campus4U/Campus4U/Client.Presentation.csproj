  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\Spaces\teretana1.jpg">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\Spaces\teretanaLogo.jpg">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\Spaces\ucionica1.jpg">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\Spaces\ucionica2.jpg">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\Spaces\ucionicaLogo.jpg">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
