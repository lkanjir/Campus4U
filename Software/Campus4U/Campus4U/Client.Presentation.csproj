<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\Spaces\teretana1.jpg" />
    <None Remove="Images\Spaces\teretanaLogo.jpg" />
    <None Remove="Images\Spaces\ucionica1.jpg" />
    <None Remove="Images\Spaces\ucionica2.jpg" />
